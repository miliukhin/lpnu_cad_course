config terminal
hostname S2
enable password cisco
enable secret class
line console 0
password cisco
login
line vty 0 15
password cisco
login
end

conf t
int vlan 1
ip address 172.16.0.3 255.255.0.0
no shutdown
exit
ip default-gateway 172.16.0.1
end

conf t
int vlan 1
ip add 172.16.0.3 255.255.0.0
no sh
end

conf t

interface fa1/1
switchport mode access

vlan 2
name fred
exit
vlan 3
name wilma
exit

int fa1/2
switchport access vlan 2
end
copy running-config startup-config

end

conf t
int Fa1/1
switchport mode trunk
