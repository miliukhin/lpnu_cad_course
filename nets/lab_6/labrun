# S1

conf t
enable password cisco
enable secret class
line console 0
password cisco
login
line vty 0 15
password cisco
login
end

conf t
int vlan 1
ip address 172.16.0.2 255.255.0.0
no shutdown
exit
ip default-gateway 172.16.0.1
end

conf t
vlan 10
name staff
exit
vlan 20
name students
exit
vlan 30
name guest
exit
vlan 99
name management
end

